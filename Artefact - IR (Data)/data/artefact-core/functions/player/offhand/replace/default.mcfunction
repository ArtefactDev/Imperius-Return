#runs if the primary player does not have any item in their offhand
#@s - primary player
#called by artefact-core:player/offhand/replace/check

#clear offhand
clear @s minecraft:stick{artefact:{offhand_placeholder:1b,item_type:["remove_offhand"]},CustomModelData:1,display:{Name:"{\"translate\":\"text.artefact.gui.attribute_status\",\"italic\":\"false\",\"color\":\"gray\"}"},HideFlags:1b}

#replace offhand
replaceitem entity @s[scores={max_hearts=0}] weapon.offhand minecraft:stick{artefact:{offhand_placeholder:1b,item_type:["remove_offhand"]},CustomModelData:1,display:{Name:"{\"translate\":\"text.artefact.gui.attribute_status\",\"italic\":\"false\",\"color\":\"gray\"}"},HideFlags:1b,AttributeModifiers:[{Slot:"offhand",AttributeName:"generic.maxHealth",Name:"generic.maxHealth",Amount:-20,Operation:0,UUIDLeast:1,UUIDMost:1}]}
